<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bluecollardev.vestracing" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vest Racing</name>
    <description>
        Racing app for getting programs and other track info.
    </description>
    <author email="lucas@bluecollardev.com" href="https://bluecollardev.com">
        Blue Collar App Development Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="APP_SECRET" value="0c1a5f95-0abd-4e71-af66-535d00535986" />
        <preference name="APPCENTER_APP_SECRET" value="0c1a5f95-0abd-4e71-af66-535d00535986" />
        <preference name="CodePushDeploymentKey" value="4B88ZS6uOgTYX9vp15znrr6gSdzCH1htBe_KV" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="~2.0.2" />
    <plugin name="cordova-plugin-fileopener" spec="~1.0.5" />
    <plugin name="cordova-plugin-inappbrowser" spec="~3.0.0" />
    <plugin name="cordova-plugin-code-push" spec="~1.11.17" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
    <plugin name="cordova-plugin-appcenter-analytics" spec="^0.3.3" />
    <plugin name="cordova-plugin-appcenter-crashes" spec="^0.3.3" />
    <plugin name="cordova-plugin-zip" spec="~3.1.0" />
</widget>
